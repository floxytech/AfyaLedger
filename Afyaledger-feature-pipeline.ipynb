"{\n \"cells\": [\n  {\n   \"cell_type\": \"markdown\",\n   \"id\": \"38eee31c\",\n   \"metadata\": {},\n   \"source\": [\n    \"# Afyaledger - Quant Feature Engineering Pipeline\\n\",\n    \"\\n\",\n    \"This notebook contains steps to compute ratios, rolling stats, z-scores and other quant features from the Afyaledger datasets.\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"id\": \"56362c21\",\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"import pandas as pd\\n\",\n    \"import numpy as np\\n\",\n    \"import os\\n\",\n    \"\\n\",\n    \"DATA_DIR = '/mnt/data'\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"id\": \"386b2e91\",\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"# Load raw statements\\n\",\n    \"income = pd.read_csv(os.path.join(DATA_DIR, 'income_statements.csv'))\\n\",\n    \"balance = pd.read_csv(os.path.join(DATA_DIR, 'balance_sheets.csv'))\\n\",\n    \"cashflow = pd.read_csv(os.path.join(DATA_DIR, 'cashflow_statements.csv'))\\n\",\n    \"market = pd.read_csv(os.path.join(DATA_DIR, 'market_data.csv'))\\n\",\n    \"\\n\",\n    \"# Ensure datetime types\\n\",\n    \"for df in [income, balance, cashflow]:\\n\",\n    \"    df['period_end'] = pd.to_datetime(df['period_end'])\\n\",\n    \"market['date'] = pd.to_datetime(market['date'])\\n\",\n    \"\\n\",\n    \"print('Loaded')\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"id\": \"0e6420a6\",\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"# --- Merge latest period-level data ---\\n\",\n    \"df = income.merge(balance, on=['company_id','period_end'], how='left')\\n\",\n    \"df = df.merge(cashflow, on=['company_id','period_end'], how='left')\\n\",\n    \"\\n\",\n    \"# --- Ratio calculations ---\\n\",\n    \"df['gross_profit'] = df['revenue'] - df['cost_of_goods_sold']\\n\",\n    \"df['gross_margin'] = df['gross_profit'] / df['revenue']\\n\",\n    \"df['operating_margin'] = df['operating_income'] / df['revenue']\\n\",\n    \"df['net_margin'] = df['net_income'] / df['revenue']\\n\",\n    \"df['ebitda_margin'] = df['ebitda'] / df['revenue']\\n\",\n    \"df['roa'] = df['net_income'] / df['total_assets']\\n\",\n    \"df['roe'] = df['net_income'] / df['shareholders_equity']\\n\",\n    \"\\n\",\n    \"# Liquidity\\n\",\n    \"# approximate total_current_assets and liabilities if columns exist\\n\",\n    \"if 'total_current_assets' not in df.columns:\\n\",\n    \"    df['total_current_assets'] = df[['cash_and_equivalents','accounts_receivable','inventory']].sum(axis=1)\\n\",\n    \"if 'total_current_liabilities' not in df.columns:\\n\",\n    \"    df['total_current_liabilities'] = df[['accounts_payable','short_term_debt']].sum(axis=1)\\n\",\n    \"\\n\",\n    \"df['current_ratio'] = df['total_current_assets'] / df['total_current_liabilities']\\n\",\n    \"df['quick_ratio'] = (df['cash_and_equivalents'] + df['accounts_receivable']) / df['total_current_liabilities']\\n\",\n    \"\\n\",\n    \"# Leverage\\n\",\n    \"df['debt_to_equity'] = (df['short_term_debt'].fillna(0) + df['long_term_debt'].fillna(0)) / df['shareholders_equity']\\n\",\n    \"df['debt_ratio'] = df['total_liabilities'] / df['total_assets']\\n\",\n    \"\\n\",\n    \"# Efficiency\\n\",\n    \"df['asset_turnover'] = df['revenue'] / df['total_assets']\\n\",\n    \"\\n\",\n    \"# Cashflow\\n\",\n    \"df['fcf_margin'] = df['free_cash_flow'] / df['revenue']\\n\",\n    \"\\n\",\n    \"print(df[['company_id','period_end','gross_margin','roe','current_ratio']].head())\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"id\": \"a3e3ce68\",\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"# --- Time-series features (rolling) ---\\n\",\n    \"# Example: compute quarter-over-quarter deltas and 4-period rolling mean per company\\n\",\n    \"\\n\",\n    \"df = df.sort_values(['company_id','period_end'])\\n\",\n    \"\\n\",\n    \"def add_rolling(group):\\n\",\n    \"    group = group.copy()\\n\",\n    \"    group['delta_revenue_q'] = group['revenue'].pct_change()\\n\",\n    \"    group['rolling_revenue_mean_4'] = group['revenue'].rolling(window=4, min_periods=1).mean()\\n\",\n    \"    group['rolling_gross_margin_mean_4'] = group['gross_margin'].rolling(window=4, min_periods=1).mean()\\n\",\n    \"    # slope of gross_margin over last 4 periods\\n\",\n    \"    def slope(arr):\\n\",\n    \"        if len(arr) < 2:\\n\",\n    \"            return np.nan\\n\",\n    \"        x = np.arange(len(arr))\\n\",\n    \"        coeffs = np.polyfit(x, arr, 1)\\n\",\n    \"        return coeffs[0]\\n\",\n    \"    group['gross_margin_slope_4'] = group['gross_margin'].rolling(window=4, min_periods=2).apply(lambda arr: slope(arr), raw=True)\\n\",\n    \"    return group\\n\",\n    \"\\n\",\n    \"df = df.groupby('company_id').apply(add_rolling).reset_index(drop=True)\\n\",\n    \"print(df[['company_id','period_end','delta_revenue_q','rolling_revenue_mean_4','gross_margin_slope_4']].head())\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"id\": \"a05e1a1c\",\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"# --- Market features: beta and volatility ---\\n\",\n    \"# compute simple returns per company\\n\",\n    \"market = market.sort_values(['company_id','date'])\\n\",\n    \"market['ret'] = market.groupby('company_id')['close'].pct_change()\\n\",\n    \"\\n\",\n    \"# merge market returns with a market index (here we approximate using market_return column provided)\\n\",\n    \"# compute rolling beta (cov/var) over 90-day window per company\\n\",\n    \"\\n\",\n    \"def rolling_beta(group):\\n\",\n    \"    group = group.copy()\\n\",\n    \"    group['beta_90d'] = group['ret'].rolling(window=90, min_periods=10).cov(group['market_return']) / group['market_return'].rolling(window=90, min_periods=10).var()\\n\",\n    \"    group['vol_30d'] = group['ret'].rolling(window=30, min_periods=5).std()\\n\",\n    \"    return group\\n\",\n    \"\\n\",\n    \"mkt = market.groupby('company_id').apply(rolling_beta).reset_index(drop=True)\\n\",\n    \"print(mkt[['company_id','date','ret','beta_90d','vol_30d']].head())\"\n   ]\n  },\n  {\n   \"cell_type\": \"code\",\n   \"execution_count\": null,\n   \"id\": \"62e7ae54\",\n   \"metadata\": {},\n   \"outputs\": [],\n   \"source\": [\n    \"# --- Export engineered features ---\\n\",\n    \"engineered = df[['company_id','period_end','gross_margin','operating_margin','net_margin','ebitda_margin','roa','roe','current_ratio','quick_ratio','debt_to_equity','debt_ratio','asset_turnover','fcf_margin','delta_revenue_q','rolling_revenue_mean_4','gross_margin_slope_4']]\\n\",\n    \"engineered.to_csv('/mnt/data/engineered_features_full.csv', index=False)\\n\",\n    \"print('Exported /mnt/data/engineered_features_full.csv')\"\n   ]\n  },\n  {\n   \"cell_type\": \"markdown\",\n   \"id\": \"b61e11e8\",\n   \"metadata\": {},\n   \"source\": [\n    \"## Notes\\n\",\n    \"- Ensure time leakages are avoided: when backtesting, only use features available at `period_end`.\\n\",\n    \"- Add z-score normalization, group ranks, and industry adjustments as needed.\"\n   ]\n  }\n ],\n \"metadata\": {},\n \"nbformat\": 4,\n \"nbformat_minor\": 5\n}"